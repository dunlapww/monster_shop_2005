<%= form_for(@user, url: "/users", :method=>'patch', :id => 'user_edit') do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :address %>
  <%= f.text_field :address %><br>
  <%= f.label :city %>
  <%= f.text_field :city %><br>
  <%= f.label :state %>
  <%= f.text_field :state %><br>
  <%= f.label :zip %>
  <%= f.text_field :zip %><br>
  <%= f.label :email_address %>
  <%= f.text_field :email_address %><br>
  <%= f.submit "Submit" %>
<% end %>
