<section class = "shipping-address">
  <h1 align = "center">Shipping Info</h1>
  <table>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
    </tr>
    <tr>
      <td><p><%= @order.user.name %> </p></td>
      <td><p><%= @order.user.address %> </p></td>
      <td><p><%= @order.user.city %> </p></td>
      <td><p><%= @order.user.state %> </p></td>
      <td><p><%= @order.user.zip %> </p></td>
    </tr>
  </table>
</section>
<center>
  <table>
    <tr>
      <th>Item</th>
      <th>Image</th>
      <th>Price</th>
      <th>Quantity</th>
    </tr>
    <% @item_orders.each do |item_order|%>
      <tr>
        <section id = "io-<%=item_order.item_order_id%>">
            <td><p><%=link_to item_order.name, "/items/#{item_order.item_id}"%></p></td>
            <td><p><%= item_order.image%></p></td>
            <td><p><%= number_to_currency(item_order.price)%></p></td>
            <td><p><%= item_order.quantity%></p></td>
        </section>
      </tr>
    <% end %>
  </table>
</center>



